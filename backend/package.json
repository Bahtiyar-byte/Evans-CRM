    "csv-parser": "^3.0.0",
    "express": "4.18.2",
    "formidable": "1.2.2",
    "helmet": "4.1.1",
    "json2csv": "^5.0.7",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "multer": "^1.4.4",
    "mysql2": "2.2.5",
    "nodemailer": "6.9.9",
    "passport": "^0.7.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "passport-microsoft": "^0.1.0",
